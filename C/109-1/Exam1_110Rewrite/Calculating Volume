#include <stdio.h>
#include <math.h>

int main(void){
    double a, b, c;
    int m;
    scanf("%d", &m);
    
    if (m==1){
    	scanf("%lf", &a);
        printf("%.2lf", pow(a,3));
    }
    
    if (m==2){
    	scanf("%lf %lf %lf", &a, &b, &c);
        printf("%.2lf", a*b*c);
    }
    
    if (m==3){
    	scanf("%lf %lf", &a, &b);
        printf("%.2lf", a*a*b*3.14);
    }
    
    if (m==4){
    	scanf("%lf", &a);
        printf("%.2lf", sqrt(2)/12*pow(a,3));
    }
    
    return 0;
}
