#include <stdio.h>

int main(void){
    int a, r, c, ar[100][100];
    scanf("%d %d %d", &a, &r, &c);
    
    for(int i=0; i<r; i++)
        for(int j=0; j<c; j++){
        	scanf("%d", &ar[i][j]);
        }
    
    switch(a){
        case 90:
            for(int j=0; j<c; j++){
                for(int i=r-1; i>=0; i--)
                { printf("%d ", ar[i][j]); }
                printf("\n");
            }
            break;
            
        case 180:
            for(int i=r-1; i>=0; i--){
            	for(int j=c-1; j>=0; j--){
                	printf("%d ", ar[i][j]); }
                printf("\n");
            }
            break;
            
        case 270:
            for(int j=c-1; j>=0; j--){
            	for(int i=0; i<r; i++){
                	printf("%d ", ar[i][j]); }
                printf("\n");
            }
            break;
    }
    
    return 0;
}
