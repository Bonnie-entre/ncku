#include <stdio.h>

int main(void){
    int m, n, a[10000];
    scanf("%d %d", &m, &n);
    
    for (int i=0; i<m+n; i++) { scanf("%d",&a[i]); }
    
    for (int i=0; i<m+n; i++)
    {
    	  int j=i;
        for(int k=i+1; k<m+n; k++)
        {
        	if(a[k]<a[j]) { j=k; }
        }
        int temp;
        temp = a[i];
        a[i] = a[j];
        a[j] = temp;
        //swap(a[i], a[j]);
    }
    
    for (int i=0; i<m+n; i++) { printf("%d ",a[i]); }
    
    return 0;
}
