#include <stdio.h>

int main(void){
    int max=0, noise;
    int a[10];
    
    for(int i=0; i<10; i++)
        scanf("%d", &a[i]);
        
    for(int i=0; i<9; i++){
        for(int j=i+1; j<10; j++){
        	noise = (a[i]+a[j])*(j-i);
          if (noise>max) max=noise;
          //printf("(%d,%d) noise:%d max:%d",i,j, noise, max);
        }
    	
    }
    printf("%d", max);
    //Noise(A, B) = (M + N) x |A - B|
    return 0;
}

