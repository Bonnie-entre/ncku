#include <stdio.h>

int main(void){
    int n1, n2, n3, n4, n5, n6, n7, n8, n9, n10, n11, n12;
    int sum, ans;
    scanf("%1d %1d%1d%1d%1d%1d%1d %1d%1d%1d%1d%1d", &n1, &n2, &n3, &n4, &n5, &n6, &n7, &n8, &n9, &n10, &n11, &n12 );
    sum = 3*(n2+n4+n6+n8+n10+n12)+n1+n3+n5+n7+n9+n11;
    ans = sum%10;
    if (ans>5){ans=10-ans;}
    printf("%d", ans);
    return 0;
}


