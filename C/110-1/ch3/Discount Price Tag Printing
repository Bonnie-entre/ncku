#include<stdio.h>

int main () {
	float a, b;
    scanf("%f %f", &a, &b);
    printf("\"\"\"\"\"\"\"\"\"\"\"\"\"\"\"\"\"\"");
    printf("\n\\    %3.0f%%off!    \\", a);  
    //注意題目敘述4space   //a的上限可以到100所以要3f
    printf("\n\\ Before  $%5.0f \\", b);
    printf("\n\\ After   $%5.2f \\", (100-a)*b/100);
    printf("\n\"\"\"\"\"\"\"\"\"\"\"\"\"\"\"\"\"\"");
    return 0;
}

