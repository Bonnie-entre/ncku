#include <stdio.h>
#define MODULUS 10007


int gcd(int a, int b)
{
    int max=a, min=b;
    if(a<b) { max=b; min=a; }
    if (min==0) { return max; } //考慮等於0
	if ( max%min == 0) { return min; }
    return gcd(min, max%min);
}

int getSecret(int n)
{
    long long end=n;
    if (n/10==0){ return n; }
    while(n/10 > 0)
    {
        //get 位數
        long long digit=0, num=n;
        do{
            digit++;
            num/=10;
        }while (num/10 != 0);
        digit++;

        //Calculate
        int u=digit/2;
        long long x,y, ans, tens=1;
        for(int i=0; i<u;i++){ tens*=10; }
        if(digit%2==1) {tens*=10;}
        x = n/tens;
        y = n%tens;
        n = gcd(x,y)%MODULUS;
        end%=MODULUS;
        end+=n;
    }
    return end%MODULUS;
}


int main(){
	int a,b;
	scanf("%d %d", &a, &b);
  //printf("%d %d", a,b);
	printf("%d %d %d", a,b, gcd(a,b));
	return 0;
}
