#include<stdio.h>

//有沒有一個可以存字串的方式？

int main () {
    int round, role, a, b, result;
    float cWin=0.00;
    
    scanf("%d", &round);
    int end[round];
    
    for (int i=0; i<round; i++)
    {
        role =0;
        result =0;
        while(role<3)
        {
            role+=1;
        	scanf("%d %d", &a, &b);
            if(a>b) { result+=1; }
            if (a<b) { result-=1; }
        }
        end[i]=result;
    }
    
    for (int i=0; i<round; i++)
    {
    	if (end[i]>0) { cWin+=1; printf("Win\n"); }   //請注意英文字母大小寫！！！
        else if (end[i]==0) { printf("Tie\n"); }
        else  { printf("Lose\n"); }
    }
    
    printf("%.2f%%", cWin/round*100);
    
    return 0;
}
