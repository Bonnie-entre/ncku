#include<stdio.h>

int main () {
    int n, cp, hp, s;
    scanf("%d %d %d %d", &n, &cp, &hp, &s);
    
    if (s>4) {n-=6 ;printf("NO\n"); return 0; }
    if (cp<100) {n-=1; }
    else if (cp>=100 && cp<=600) 
    {
        n-=5;
        if (hp>100) {n-=3;}
    }
    else 
    {
        n-=15;
        if (hp>200) {n-=10;}
    }
    
    if (n>=0) {printf("YES\n%d", n);}
    else {printf("NO\n");}
    
    return 0;
}

