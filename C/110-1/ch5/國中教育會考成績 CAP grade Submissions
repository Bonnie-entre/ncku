#include<stdio.h>

int main () {
    int c, sc, so, jp=0, js=0, cA=0, cB=0, cC=0;
    float e, m;
    scanf("%d %f %f %d %d", &c, &e, &m, &so, &sc);
    
    //Chinese
    if (c>=41) 
    {
        jp+=6;
        cA+=1;
        if (c>=45) {js+=7;}
        else if (c<=42) {js+=5;}
        else {js+=6;}
    }
    else if (c>=19 && c<=40) 
    {
        jp+=4;
        cB+=1;
        if (c>=36) {js+=4;}
        else if (c<=30) {js+=2;}
        else {js+=3;}
    }
    else {cC+=1; jp+=2; js+=1;}
    printf("Chinese A:%d B:%d C:%d\n", cA, cB, cC);
    

    //English
    if (e>=90.24) 
    {
        jp+=6;
        cA+=1;
        if (e>=98.05) {js+=7;}
        else if (e<=94.29) {js+=5;}
        else {js+=6;}
    }
    else if (e>=38.75 && e<=89.52) 
    {
        jp+=4;
        cB+=1;
        if (e>=81.58) {js+=4;}
        else if (e<=69.78) {js+=2;}
        else {js+=3;}
    }
    else {cC+=1; jp+=2; js+=1;}
    printf("English A:%d B:%d C:%d\n", cA, cB, cC);
    
    //Math
    if (m>=81.73) 
    {
        jp+=6;
        cA+=1;
        if (m>=94.23) {js+=7;}
        else if (m<=90.00) {js+=5;}
        else {js+=6;}
    }
    else if (m>=40.96 && m<=81.15) 
    {
        jp+=4;
        cB+=1;
        if (m>=72.12) {js+=4;}
        else if (m<=62.31) {js+=2;}
        else {js+=3;}
    }
    else {cC+=1; jp+=2; js+=1;}
    printf("Math A:%d B:%d C:%d\n", cA, cB, cC);
    
    //Science
    if (sc>=46) 
    {
        jp+=6;
        cA+=1;
        if (sc>=52) {js+=7;}
        else if (sc<=49) {js+=5;}
        else {js+=6;}
    }
    else if (sc>=20 && sc<=45) 
    {
        jp+=4;
        cB+=1;
        if (sc>=38) {js+=4;}
        else if (sc<=30) {js+=2;}
        else {js+=3;}
    }
    else {cC+=1; jp+=2; js+=1;}
    printf("Science A:%d B:%d C:%d\n", cA, cB, cC);
    
    //Social
    if (so>=56) 
    {
        jp+=6;
        cA+=1;
        if (so>=61) {js+=7;}
        else if (so<=58) {js+=5;}
        else {js+=6;}
    }
    else if (so>=23 && so<=55) 
    {
        jp+=4;
        cB+=1;
        if (so>=48) {js+=4;}
        else if (so<=38) {js+=2;}
        else {js+=3;}
    }
    else {cC+=1; jp+=2; js+=1;}

    

    printf("%d/%d(", jp, js);
    if( cA>0) {printf("%dA", cA);}
    if( cB>0) {printf("%dB", cB);}
    if( cC>0) {printf("%dC", cC);}
    printf(")");
    
    return 0;
}

