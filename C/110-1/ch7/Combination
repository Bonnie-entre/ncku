#include <stdio.h>

int main(void){
    int m, n;
    long long ans=1;
    
    scanf("%d %d", &m, &n);
    if (m==n | n==0) {
    	printf("1");
        return 0;
    }
    
    //要記得邊*邊/   //參數上下限
    int count=1;
    for (int i=m; i>=n+1; i--)
	    {
        ans *=i; 
        if (count<=m-n){
          ans /= count;
          count ++;
        }
      }
    printf("%lld", ans);
    
    return 0;
}


//在測試時，用input(上下限)極值去試

